    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prompts": "node ./scripts/fetch-prompts.mjs",
    "prepare": "husky install",
    "proxy-dev": "sh ./scripts/init-proxy.sh && proxychains -f ./scripts/proxychains.conf yarn dev",
    "typechain:generate": "yarn typechain:generate:web3 && yarn typechain:generate:ethers",
    "typechain:generate:web3": "typechain --target=web3-v1 'app/web3/blockchain/abi/*.json'",
    "typechain:generate:ethers": "typechain --target=ethers-v5 'app/web3/blockchain/abi/*.json'",
    "postinstall": "rm -f node_modules/web3/index.d.ts && yarn patch-package && yarn typechain:generate && yarn xstate:typegen",
    "xstate:typegen": "xstate typegen \"@(features|pages|components|utils)/**/*.ts?(x)\" ",
    "xstate:typegen:watch": "xstate typegen \"@(features|pages|components|utils)/**/*.ts?(x)\" --watch"
  },
